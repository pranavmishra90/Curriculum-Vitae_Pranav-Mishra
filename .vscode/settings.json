    "latex-workshop.intellisense.biblatexJSON.replace": {},
  
    "latex-workshop.latex.tools": [
    
      {
        "name": "xelatex",
        "command": "xelatex",
        "args": [
          "-synctex=1",
          "-interaction=nonstopmode",
          "-file-line-error",
          "%DOC%"
        ],
  
        "env": {
          "TEXMFHOME": "C:/texlive/2022/bin/win32/latex"
        }
      },
      {
        "name": "biber",
        "command": "biber",
        "args": ["%DOCFILE%"]
      },
      {
        "name": "bibtex",
        "command": "bibtex",
        "args": ["%DOCFILE%"]
      },
      {
        "name": "latexmk",
        "tools": ["latexmk"]
      },
      {
        "name": "pdflatex",
        "command": "pdflatex",
        "args": [
          "-synctex=1",
          "-interaction=nonstopmode",
          "-file-line-error",
          "%DOC%"
        ]
      },
      {
        "name": "pdflatex",
        "command": "pdflatex",
        "args": [
          "-synctex=1",
          "-interaction=nonstopmode",
          "-file-line-error",
          "%DOC%"
        ]
      }
    ],
  
